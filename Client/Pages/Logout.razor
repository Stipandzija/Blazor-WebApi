@page "/logout"
@inject BlazorClient.Services.AuthService authService

@code {
    protected override async Task OnInitializedAsync()
    {
        try
        {
            if (authService != null)
            {
                await authService.LogoutAsync();
            }
            else
            {
                Console.WriteLine("AuthService is null");
            }
        }
        catch (Exception ex)
        {
            Console.WriteLine($"An error occurred while logging out: {ex.Message}");
        }
    }
}
